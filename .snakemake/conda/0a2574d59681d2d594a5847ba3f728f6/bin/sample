#!/home/simone/mnt/part4/keygene2/tn/SIMONE/.snakemake/conda/0a2574d59681d2d594a5847ba3f728f6/bin/python
# EASY-INSTALL-ENTRY-SCRIPT: 'mmtf-python==1.0.2','console_scripts','sample'
__requires__ = 'mmtf-python==1.0.2'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('mmtf-python==1.0.2', 'console_scripts', 'sample')()
    )
